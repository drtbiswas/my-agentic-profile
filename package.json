{
    "name": "@agentic-profile/a2a-mcp-express",
    "version": "0.7.2",
    "description": "A2A (Agent-to-Agent) and MCP Express middleware for AI Agents using A2A, MCP, and the Agentic Profile protocols",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prepare": "tsup",
        "build": "tsup",
        "test": "jest",
        "dev": "tsx --inspect test/server/index.local.ts",
        "auth": "tsx test/authenticate.ts",
        "chat": "tsx test/hello-chat.ts"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./a2a-service": {
            "types": "./dist/a2a-service/index.d.ts",
            "import": "./dist/a2a-service/index.js",
            "require": "./dist/a2a-service/index.cjs"
        },
        "./mcp-service": {
            "types": "./dist/mcp-service/index.d.ts",
            "import": "./dist/mcp-service/index.js",
            "require": "./dist/mcp-service/index.cjs"
        },
        "./json-rpc-client": {
            "types": "./dist/json-rpc/json-rpc-client/index.d.ts",
            "import": "./dist/json-rpc/json-rpc-client/index.js",
            "require": "./dist/json-rpc/json-rpc-client/index.cjs"
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/agentic-profile/agentic-profile-a2a-mcp-express.git"
    },
    "author": "Mike Prince",
    "license": "Apache-2",
    "bugs": {
        "url": "https://github.com/agentic-profile/agentic-profile-a2a-mcp-express/issues"
    },
    "homepage": "https://github.com/agentic-profile/agentic-profile-a2a-mcp-express#readme",
    "dependencies": {
        "@agentic-profile/auth": "^0.7.0",
        "@agentic-profile/common": "^0.7.0",
        "did-resolver": "^4.1.0",
        "loglevel": "^1.9.2",
        "uuid": "^13.0.0"
    },
    "peerDependencies": {
        "@a2a-js/sdk": "^0.3.10",
        "@modelcontextprotocol/sdk": "^1.26.0",
        "express": "^5.2.1"
    },
    "peerDependenciesMeta": {
        "@a2a-js/sdk": {
            "optional": true
        },
        "@modelcontextprotocol/sdk": {
            "optional": true
        },
        "express": {
            "optional": true
        }
    },
    "devDependencies": {
        "@a2a-js/sdk": "^0.3.10",
        "@google/genai": "^1.38.0",
        "@modelcontextprotocol/sdk": "^1.26.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.17",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "ts-node": "^10.9.2",
        "tsup": "^8.4.0",
        "tsx": "^4.20.3"
    },
    "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
